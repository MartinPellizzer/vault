{"PubmedArticle": [{"MedlineCitation": {"SpaceFlightMission": [], "OtherID": [], "InvestigatorList": [], "OtherAbstract": [], "CitationSubset": ["IM"], "KeywordList": [], "GeneralNote": [], "PMID": "17269527", "DateCompleted": {"Year": "2007", "Month": "02", "Day": "15"}, "DateRevised": {"Year": "2016", "Month": "11", "Day": "24"}, "Article": {"ArticleDate": [], "Language": ["eng"], "ELocationID": [], "Journal": {"ISSN": "0038-4348", "JournalIssue": {"Volume": "100", "Issue": "1", "PubDate": {"Year": "2007", "Month": "Jan"}}, "Title": "Southern medical journal", "ISOAbbreviation": "South Med J"}, "ArticleTitle": "Pancreatitis in a woman taking an herbal supplement.", "Pagination": {"StartPage": "59", "EndPage": "60", "MedlinePgn": "59-60"}, "Abstract": {"AbstractText": ["A healthy woman developed pancreatitis after starting a popular herbal supplement designed to boost female libido. Following cessation of the herbal supplement, her enzymes quickly normalized. The patient had no risk factors for pancreatitis and anatomic obstruction was later ruled out. The contents of the supplement and their history of side effects are reviewed. As several of the ingredients are estrogenic in nature, this may have contributed to the patient's reaction. Physicians should remember to take a careful history regarding the usage of supplements and alternative medicines."]}, "AuthorList": [{"AffiliationInfo": [{"Identifier": [], "Affiliation": "US Naval Hospital, 6000 Highway 98 West, Pensacola, FL 32512, USA. david.s.lesser@pcola.med.navy.mil"}], "Identifier": [], "LastName": "Lesser", "ForeName": "David", "Initials": "D"}, {"AffiliationInfo": [], "Identifier": [], "LastName": "Hillesheim", "ForeName": "Paul", "Initials": "P"}], "PublicationTypeList": ["Case Reports", "Journal Article"]}, "MedlineJournalInfo": {"Country": "United States", "MedlineTA": "South Med J", "NlmUniqueID": "0404522", "ISSNLinking": "0038-4348"}, "ChemicalList": [{"RegistryNumber": "0", "NameOfSubstance": "Phytoestrogens"}], "CommentsCorrectionsList": [{"RefSource": "South Med J. 2007 Jan;100(1):89.", "PMID": "17269540"}, {"RefSource": "South Med J. 2007 Jun;100(6):614-5. doi: 10.1097/smj.0b013e3180600e06", "PMID": "17591320"}], "MeshHeadingList": [{"QualifierName": [], "DescriptorName": "Adult"}, {"QualifierName": [], "DescriptorName": "Female"}, {"QualifierName": [], "DescriptorName": "Follow-Up Studies"}, {"QualifierName": [], "DescriptorName": "Humans"}, {"QualifierName": ["drug effects"], "DescriptorName": "Libido"}, {"QualifierName": ["chemically induced", "diagnostic imaging"], "DescriptorName": "Pancreatitis"}, {"QualifierName": ["adverse effects"], "DescriptorName": "Phytoestrogens"}, {"QualifierName": ["adverse effects"], "DescriptorName": "Plants, Medicinal"}, {"QualifierName": [], "DescriptorName": "Tomography, X-Ray Computed"}]}, "PubmedData": {"ReferenceList": [], "History": [{"Year": "2007", "Month": "2", "Day": "3", "Hour": "9", "Minute": "0"}, {"Year": "2007", "Month": "2", "Day": "16", "Hour": "9", "Minute": "0"}, {"Year": "2007", "Month": "2", "Day": "3", "Hour": "9", "Minute": "0"}], "PublicationStatus": "ppublish", "ArticleIdList": ["17269527", "10.1097/SMJ.0b013e31802e48cb"]}}], "PubmedBookArticle": []}