{"PubmedBookArticle": [], "PubmedArticle": [{"MedlineCitation": {"OtherAbstract": [], "CitationSubset": ["IM"], "OtherID": [], "GeneralNote": [], "SpaceFlightMission": [], "KeywordList": [], "InvestigatorList": [], "PMID": "265084", "DateCompleted": {"Year": "1977", "Month": "04", "Day": "25"}, "DateRevised": {"Year": "2023", "Month": "12", "Day": "13"}, "Article": {"Language": ["eng"], "ArticleDate": [], "ELocationID": [], "Journal": {"ISSN": "0029-845X", "JournalIssue": {"Volume": "85", "Issue": "1", "PubDate": {"Year": "1977", "Month": "Jan"}}, "Title": "Scandinavian journal of dental research", "ISOAbbreviation": "Scand J Dent Res"}, "ArticleTitle": "The relation between betel chewing and dental caries.", "Pagination": {"StartPage": "64", "EndPage": "70", "MedlinePgn": "64-70"}, "Abstract": {"AbstractText": ["In the eastern part of Java and on the island of Bali betel chewing is a very common habit confined principally to females. The purpose of the present study was to examine the influence of betel chewing on the prevalence of dental caries. The study, which is based upon 982 villagers, indicates a strong inverse relationship between the prevalence of dental caries and the intensity of betel chewing. The possible mechanisms whereby betel chewing may have a caries-inhibiting effect are discussed."]}, "AuthorList": [{"Identifier": [], "AffiliationInfo": [], "LastName": "M\u00f6ller", "ForeName": "I J", "Initials": "IJ"}, {"Identifier": [], "AffiliationInfo": [], "LastName": "Pindborg", "ForeName": "J J", "Initials": "JJ"}, {"Identifier": [], "AffiliationInfo": [], "LastName": "Effendi", "ForeName": "I", "Initials": "I"}], "PublicationTypeList": ["Journal Article"]}, "MedlineJournalInfo": {"Country": "Denmark", "MedlineTA": "Scand J Dent Res", "NlmUniqueID": "0270023", "ISSNLinking": "0029-845X"}, "MeshHeadingList": [{"QualifierName": [], "DescriptorName": "Adolescent"}, {"QualifierName": [], "DescriptorName": "Adult"}, {"QualifierName": [], "DescriptorName": "Age Factors"}, {"QualifierName": [], "DescriptorName": "Areca"}, {"QualifierName": [], "DescriptorName": "DMF Index"}, {"QualifierName": ["epidemiology"], "DescriptorName": "Dental Caries"}, {"QualifierName": [], "DescriptorName": "Female"}, {"QualifierName": [], "DescriptorName": "Humans"}, {"QualifierName": [], "DescriptorName": "Indonesia"}, {"QualifierName": [], "DescriptorName": "Male"}, {"QualifierName": [], "DescriptorName": "Mastication"}, {"QualifierName": [], "DescriptorName": "Middle Aged"}, {"QualifierName": [], "DescriptorName": "Plants, Medicinal"}, {"QualifierName": [], "DescriptorName": "Plants, Toxic"}, {"QualifierName": [], "DescriptorName": "Regression Analysis"}, {"QualifierName": [], "DescriptorName": "Sex Factors"}, {"QualifierName": [], "DescriptorName": "Nicotiana"}]}, "PubmedData": {"ReferenceList": [], "History": [{"Year": "1977", "Month": "1", "Day": "1"}, {"Year": "1977", "Month": "1", "Day": "1", "Hour": "0", "Minute": "1"}, {"Year": "1977", "Month": "1", "Day": "1", "Hour": "0", "Minute": "0"}], "PublicationStatus": "ppublish", "ArticleIdList": ["265084", "10.1111/j.1600-0722.1977.tb00534.x"]}}]}