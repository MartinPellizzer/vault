{"PubmedBookArticle": [], "PubmedArticle": [{"MedlineCitation": {"OtherAbstract": [], "CitationSubset": ["IM"], "OtherID": [], "GeneralNote": [], "SpaceFlightMission": [], "KeywordList": [], "InvestigatorList": [], "PMID": "1636566", "DateCompleted": {"Year": "1992", "Month": "08", "Day": "21"}, "DateRevised": {"Year": "2013", "Month": "11", "Day": "21"}, "Article": {"Language": ["eng"], "ArticleDate": [], "ELocationID": [], "Journal": {"ISSN": "0002-838X", "JournalIssue": {"Volume": "46", "Issue": "2", "PubDate": {"Year": "1992", "Month": "Aug"}}, "Title": "American family physician", "ISOAbbreviation": "Am Fam Physician"}, "ArticleTitle": "Jimson weed toxicity: management of anticholinergic plant ingestion.", "Pagination": {"StartPage": "526", "EndPage": "530", "MedlinePgn": "526-30"}, "Abstract": {"AbstractText": ["Jimson weed is a hallucinogenic plant that is common in rural areas. Consumption of any part of the plant can result in severe anticholinergic toxicity. The clinical presentation of jimson weed toxicity is similar to that seen in cases of atropine poisoning. Treatment is aimed at removing plant material from the gastrointestinal tract, keeping the patient safe and reversing severe anticholinergic sequelae."]}, "AuthorList": [{"Identifier": [], "AffiliationInfo": [{"Identifier": [], "Affiliation": "Department of Family Medicine, York Hospital, PA."}], "LastName": "Vanderhoff", "ForeName": "B T", "Initials": "BT"}, {"Identifier": [], "AffiliationInfo": [], "LastName": "Mosser", "ForeName": "K H", "Initials": "KH"}], "PublicationTypeList": ["Case Reports", "Journal Article", "Review"]}, "MedlineJournalInfo": {"Country": "United States", "MedlineTA": "Am Fam Physician", "NlmUniqueID": "1272646", "ISSNLinking": "0002-838X"}, "ChemicalList": [{"RegistryNumber": "0", "NameOfSubstance": "Emetics"}, {"RegistryNumber": "16291-96-6", "NameOfSubstance": "Charcoal"}, {"RegistryNumber": "7C0697DR9I", "NameOfSubstance": "Atropine"}, {"RegistryNumber": "9U1VM840SP", "