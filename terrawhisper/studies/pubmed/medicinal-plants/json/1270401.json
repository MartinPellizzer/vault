{"PubmedBookArticle": [], "PubmedArticle": [{"MedlineCitation": {"OtherAbstract": [], "CitationSubset": ["IM"], "OtherID": [], "GeneralNote": [], "SpaceFlightMission": [], "KeywordList": [], "InvestigatorList": [], "PMID": "1270401", "DateCompleted": {"Year": "1976", "Month": "08", "Day": "02"}, "DateRevised": {"Year": "2003", "Month": "11", "Day": "14"}, "Article": {"Language": ["eng"], "ArticleDate": [], "ELocationID": [], "Journal": {"ISSN": "0004-5756", "JournalIssue": {"Volume": "59", "Issue": "3", "PubDate": {"Year": "1976", "Month": "May"}}, "Title": "Journal - Association of Official Analytical Chemists", "ISOAbbreviation": "J Assoc Off Anal Chem"}, "ArticleTitle": "Spectrophotometric determination of allylisothiocyanate in mustard seed oil.", "Pagination": {"StartPage": "689", "EndPage": "692", "MedlinePgn": "689-92"}, "Abstract": {"AbstractText": ["Allylisothiocyanate is determined spectrophotometrically after reaction with 2,3-dichloro-1,4-naphthoquinone. For pure samples, the color intensity is proportional to allylisothiocyanate content in the range 0.8-3.0 mg/ml reaction mixture. A modified procedure is used to estimate allylisothiocyanate content of mustard seed oil. The reaction is linear for allylisothiocyanate concentrations in the range 40-240 mug/ml reaction mixture. Two mustard seed oil samples contained 0.995 not equal to 0.020 and 0.981 not equal to 0.019% allylisothiocyanate."]}, "AuthorList": [{"Identifier": [], "AffiliationInfo": [], "LastName": "Devani", "ForeName": "M B", "Initials": "MB"}, {"Identifier": [], "AffiliationInfo": [], "LastName": "Shishoo", "ForeName": "C J", "Initials": "CJ"}, {"Identifier": [], "AffiliationInfo": [], "LastName": "Dadia", "ForeName": "B K", "Initials": "BK"}], "PublicationTypeList": ["Journal Article"]}, "MedlineJournalInfo": {"Country": "United States", "MedlineTA": "J Assoc Off Anal Chem", "NlmUniqueID": "7505559", "ISSNLinking": "0004-5756"}, "ChemicalList": [{"RegistryNumber": "0", "NameOfSubstance": "Oils"}, {"RegistryNumber": "0", "NameOfSubstance": "Thiocyanates"}], "MeshHeadingList": [{"QualifierName": ["analysis"], "DescriptorName": "Mustard Plant"}, {"QualifierName": ["analysis"], "DescriptorName": "Oils"}, {"QualifierName": [], "DescriptorName": "Plants, Medicinal"}, {"QualifierName": ["analysis"], "DescriptorName": "Seeds"}, {"QualifierName": [], "DescriptorName": "Spectrophotometry"}, {"QualifierName": ["analysis"], "DescriptorName": "Thiocyanates"}]}, "PubmedData": {"ReferenceList": [], "History": [{"Year": "1976", "Month": "5", "Day": "1"}, {"Year": "1976", "Month": "5", "Day": "1", "Hour": "0", "Minute": "1"}, {"Year": "1976", "Month": "5", "Day": "1", "Hour": "0", "Minute": "0"}], "PublicationStatus": "ppublish", "ArticleIdList": ["1270401"]}}]}