{"PubmedBookArticle": [], "PubmedArticle": [{"MedlineCitation": {"OtherAbstract": [], "CitationSubset": ["IM"], "OtherID": [], "GeneralNote": [], "SpaceFlightMission": [], "KeywordList": [], "InvestigatorList": [], "PMID": "7319945", "DateCompleted": {"Year": "1982", "Month": "03", "Day": "22"}, "DateRevised": {"Year": "2019", "Month": "05", "Day": "12"}, "Article": {"Language": ["eng"], "ArticleDate": [], "ELocationID": [], "Journal": {"ISSN": "0021-8812", "JournalIssue": {"Volume": "53", "Issue": "2", "PubDate": {"Year": "1981", "Month": "Aug"}}, "Title": "Journal of animal science", "ISOAbbreviation": "J Anim Sci"}, "ArticleTitle": "Intestinal absorption of D-glucose, D-galactose and L-leucine in male growing rats fed a raw field bean (Vicia faba L.) diet.", "Pagination": {"StartPage": "414", "EndPage": "419", "MedlinePgn": "414-9"}, "Abstract": {"AbstractText": ["Male growing rats were fed diets containing either raw field bean (Vicia faba L.) or casein as the only source of protein. Diets were fed ad libitum for 5 weeks. Body weight gain was significantly impaired, and rates of both in vivo and in vitro intestinal absorption of D-glucose, D-galactose and L-leucine were reduced, in rats fed the legume diet. The addition of methionine to the bean diet did not alter either growth or intestinal absorption rates. No significant differences in intestinal oxygen uptake were found between the treatment groups. It is suggested that the antinutritive substances contained in the raw legume Vicia faba are responsible for the inhibitory effects observed in this experiment."]}, "AuthorList": [{"Identifier": [], "AffiliationInfo": [], "LastName": "Santidrian", "ForeName": "S", "Initials": "S"}], "PublicationTypeList": ["Journal Article"]}, "MedlineJournalInfo": {"Country": "United States", "MedlineTA": "J Anim Sci", "NlmUniqueID": "8003002", "ISSNLinking": "0021-8812"}, "ChemicalList": [{"RegistryNumber": "GMW67QNF9C", "NameOfSubstance": "Leucine"}, {"RegistryNumber": "IY9XDZ35W2", "NameOfSubstance": "Glucose"}, {"RegistryNumber": "X2RN3Q8DNE", "NameOfSubstance": "Galactose"}], "MeshHeadingList": [{"QualifierName": [], "DescriptorName": "Animal Feed"}, {"QualifierName": [], "DescriptorName": "Animals"}, {"QualifierName": [], "DescriptorName": "Fabaceae"}, {"QualifierName": ["metabolism"], "DescriptorName": "Galactose"}, {"QualifierName": ["metabolism"], "DescriptorName": "Glucose"}, {"QualifierName": [], "DescriptorName": "Intestinal Absorption"}, {"QualifierName": ["metabolism"], "DescriptorName": "Leucine"}, {"QualifierName": [], "DescriptorName": "Male"}, {"QualifierName": [], "DescriptorName": "Plants, Medicinal"}, {"QualifierName": [], "DescriptorName": "Rats"}, {"QualifierName": ["metabolism"], "DescriptorName": "Rats, Inbred Strains"}]}, "PubmedData": {"ReferenceList": [], "History": [{"Year": "1981", "Month": "8", "Day": "1"}, {"Year": "1981", "Month": "8", "Day": "1", "Hour": "0", "Minute": "1"}, {"Year": "1981", "Month": "8", "Day": "1", "Hour": "0", "Minute": "0"}], "PublicationStatus": "ppublish", "ArticleIdList": ["7319945", "10.2527/jas1981.532414x"]}}]}