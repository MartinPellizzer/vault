{"PubmedBookArticle": [], "PubmedArticle": [{"MedlineCitation": {"OtherAbstract": [], "CitationSubset": ["IM"], "OtherID": [], "GeneralNote": [], "SpaceFlightMission": [], "KeywordList": [], "InvestigatorList": [], "PMID": "9448809", "DateCompleted": {"Year": "1998", "Month": "02", "Day": "18"}, "DateRevised": {"Year": "2022", "Month": "03", "Day": "18"}, "Article": {"Language": ["eng"], "ArticleDate": [], "ELocationID": [], "Journal": {"ISSN": "0020-6539", "JournalIssue": {"Volume": "47", "Issue": "5", "PubDate": {"Year": "1997", "Month": "Oct"}}, "Title": "International dental journal", "ISOAbbreviation": "Int Dent J"}, "ArticleTitle": "Chewing sticks and the oral hygiene habits of the adult Pakistani population.", "Pagination": {"StartPage": "275", "EndPage": "278", "MedlinePgn": "275-8"}, "Abstract": {"AbstractText": ["This paper discusses the oral hygiene habits of the adult Pakistani population with special reference to chewing sticks. Information collected during the National Health Survey of Pakistan (1990-1994) showed that in the rural areas more than 50 per cent of the population used chewing sticks or miswak as a tool for oral hygiene. Conversely, in the urban population the majority use a toothbrush. About 36 per cent of the Pakistani population clean their teeth daily, irrespective of the method employed. Eight per cent of the 120 million population never clean their teeth, while 54 per cent clean their teeth either on alternate days, weekly or monthly."]}, "AuthorList": [{"Identifier": [], "AffiliationInfo": [], "LastName": "Asadi", "ForeName": "S G", "Initials": "SG"}, {"Identifier": [], "AffiliationInfo": [], "LastName": "Asadi", "ForeName": "Z G", "Initials": "ZG"}], "PublicationTypeList": ["Journal Article"]}, "MedlineJournalInfo": {"Country": "England", "MedlineTA": "Int Dent J", "NlmUniqueID": "0374714", "ISSNLinking": "0020-6539"}, "MeshHeadingList": [{"QualifierName": [], "DescriptorName": "Adult"}, {"QualifierName": [], "DescriptorName": "Humans"}, {"QualifierName": [], "DescriptorName": "Medicine, Traditional"}, {"QualifierName": ["instrumentation", "statistics & numerical data"], "DescriptorName": "Oral Hygiene"}, {"QualifierName": ["epidemiology"], "DescriptorName": "Pakistan"}, {"QualifierName": [], "DescriptorName": "Plants, Medicinal"}, {"QualifierName": ["statistics & numerical data"], "DescriptorName": "Rural Health"}, {"QualifierName": ["instrumentation", "methods", "statistics & numerical data"], "DescriptorName": "Toothbrushing"}, {"QualifierName": ["statistics & numerical data"], "DescriptorName": "Urban Health"}]}, "PubmedData": {"ReferenceList": [], "History": [{"Year": "1998", "Month": "2", "Day": "4"}, {"Year": "1998", "Month": "2", "Day": "4", "Hour": "0", "Minute": "1"}, {"Year": "1998", "Month": "2", "Day": "4", "Hour": "0", "Minute": "0"}], "PublicationStatus": "ppublish", "ArticleIdList": ["9448809", "10.1002/j.1875-595x.1997.tb00789.x"]}}]}