{"PubmedArticle": [{"MedlineCitation": {"SpaceFlightMission": [], "OtherID": [], "InvestigatorList": [], "OtherAbstract": [], "CitationSubset": ["IM"], "KeywordList": [], "GeneralNote": [], "PMID": "19057813", "DateCompleted": {"Year": "2009", "Month": "05", "Day": "05"}, "DateRevised": {"Year": "2019", "Month": "06", "Day": "06"}, "Article": {"ArticleDate": [], "Language": ["eng"], "ELocationID": ["S0074-02762008000700004"], "Journal": {"ISSN": "1678-8060", "JournalIssue": {"Volume": "103", "Issue": "7", "PubDate": {"Year": "2008", "Month": "Nov"}}, "Title": "Memorias do Instituto Oswaldo Cruz", "ISOAbbreviation": "Mem Inst Oswaldo Cruz"}, "ArticleTitle": "Antibacterial and cytotoxic activity of Kenyan medicinal plants.", "Pagination": {"StartPage": "650", "EndPage": "652", "MedlinePgn": "650-2"}, "Abstract": {"AbstractText": ["Seven medicinal plant extracts traditionally used in Kenya, mainly for management of infectious conditions, were chosen and screened for their antibacterial activity against Gram-negative (Pseudomonas aeruginosa and Escherichia coli) and Gram-positive (Bacillus cereus and Staphylococcus aureus) bacteria. Antibacterial activity was tested using the broth dilution method. Harrisonia abyssinica and Terminalia kilimandscharica extracts showed significant activity against Gram+ and Gram- bacteria. The methanolic extracts of T. kilimandscharica bark and H. abyssinica bark and leaves showed minimum inhibitory activity against all tested bacteria, with minimal inhibitory concentrations ranging from 25-150 mg/mL. Ajuga remota and Amaranthus hybridus, which are lethal to brine shrimp nauplii, showed significantly lower antibacterial activity than those that were relatively non-toxic."]}, "AuthorList": [{"AffiliationInfo": [{"Identifier": [], "Affiliation": "Department of Public Health, Pharmacology and Toxicology, University of Nairobi, PO Box 29053, 00625 Nairobi, Kenya. gwagate@yahoo.com"}], "Identifier": [], "LastName": "Cyrus", "ForeName": "Wagate G", "Initials": "WG"}, {"AffiliationInfo": [], "Identifier": [], "LastName": "Daniel", "ForeName": "Gakuya W", "Initials": "GW"}, {"AffiliationInfo": [], "Identifier": [], "LastName": "Nanyingi", "ForeName": "Mark O", "Initials": "MO"}, {"AffiliationInfo": [], "Identifier": [], "LastName": "Njonge", "ForeName": "Francis K", "Initials": "FK"}, {"AffiliationInfo": [], "Identifier": [], "LastName": "Mbaria", "ForeName": "James M", "Initials": "JM"}], "PublicationTypeList": ["Journal Article", "Research Support, Non-U.S. Gov't"]}, "MedlineJournalInfo": {"Country": "Brazil", "MedlineTA": "Mem Inst Oswaldo Cruz", "NlmUniqueID": "7502619", "ISSNLinking": "0074-0276"}, "ChemicalList": [{"RegistryNumber": "0", "NameOfSubstance": "Anti-Bacterial Agents"}, {"RegistryNumber": "0", "NameOfSubstance": "Plant Extracts"}], "MeshHeadingList": [{"QualifierName": [], "DescriptorName": "Animals"}, {"QualifierName": ["pharmacology", "toxicity"], "DescriptorName": "Anti-Bacterial Agents"}, {"QualifierName": ["drug effects"], "DescriptorName": "Artemia"}, {"QualifierName": ["drug effects"], "DescriptorName": "Gram-Negative Bacteria"}, {"QualifierName": ["drug effects"], "DescriptorName": "Gram-Positive Bacteria"}, {"QualifierName": [], "DescriptorName": "Kenya"}, {"QualifierName": [], "DescriptorName": "Lethal Dose 50"}, {"QualifierName": [], "DescriptorName": "Microbial Sensitivity Tests"}, {"QualifierName": ["pharmacology"], "DescriptorName": "Plant Extracts"}, {"QualifierName": ["chemistry", "classification", "toxicity"], "DescriptorName": "Plants, Medicinal"}]}, "PubmedData": {"ReferenceList": [], "History": [{"Year": "2008", "Month": "4", "Day": "3"}, {"Year": "2008", "Month": "9", "Day": "16"}, {"Year": "2008", "Month": "12", "Day": "6", "Hour": "9", "Minute": "0"}, {"Year": "2009", "Month": "5", "Day": "6", "Hour": "9", "Minute": "0"}, {"Year": "2008", "Month": "12", "Day": "6", "Hour": "9", "Minute": "0"}], "PublicationStatus": "ppublish", "ArticleIdList": ["19057813", "10.1590/s