{"PubmedBookArticle": [], "PubmedArticle": [{"MedlineCitation": {"OtherAbstract": [], "CitationSubset": ["IM"], "OtherID": [], "GeneralNote": [], "SpaceFlightMission": [], "KeywordList": [], "InvestigatorList": [], "PMID": "1726075", "DateCompleted": {"Year": "1992", "Month": "06", "Day": "19"}, "DateRevised": {"Year": "2019", "Month": "07", "Day": "05"}, "Article": {"Language": ["eng"], "ArticleDate": [], "ELocationID": [], "Journal": {"ISSN": "0009-2363", "JournalIssue": {"Volume": "39", "Issue": "12", "PubDate": {"Year": "1991", "Month": "Dec"}}, "Title": "Chemical & pharmaceutical bulletin", "ISOAbbreviation": "Chem Pharm Bull (Tokyo)"}, "ArticleTitle": "Biologically active constituents of Centipeda minima: sesquiterpenes of potential anti-allergy activity.", "Pagination": {"StartPage": "3272", "EndPage": "3275", "MedlinePgn": "3272-5"}, "Abstract": {"AbstractText": ["Ether, methanol and aqueous extracts of Centipeda minima (Compositae) herbs were found to have significant anti-allergy activities in passive cutaneous anaphylaxis (PCA) test. Three flavonoids, two sesquiterpene lactones and an amide were isolated from this plant material as inhibitors to induced histamine release from mast cells. The sesquiterpenes were identified as isobutyroylplenolin and senecioylplenolin by spectral investigations. The flavonoids and sesquiterpenes exhibited significant anti-allergy activity in PCA test with p.o. administration."]}, "AuthorList": [{"Identifier": [], "AffiliationInfo": [{"Identifier": [], "Affiliation": "Faculty of Pharmaceutical Sciences, University of Tokyo, Japan."}], "LastName": "Wu", "ForeName": "J B", "Initials": "JB"}, {"Identifier": [], "AffiliationInfo": [], "LastName": "Chun", "ForeName": "Y T", "Initials": "YT"}, {"Identifier": [], "AffiliationInfo": [], "LastName": "Ebizuka", "ForeName": "Y", "Initials": "Y"}, {"Identifier": [], "AffiliationInfo": [], "LastName": "Sankawa", "ForeName": "U", "Initials": "U"}], "PublicationTypeList": ["Journal Article", "Research Support, Non-U.S. Gov't"]}, "MedlineJournalInfo": {"Country": "Japan", "MedlineTA": "Chem Pharm Bull (Tokyo)", "NlmUniqueID": "0377775", "ISSNLinking": "0009-2363"}, "ChemicalList": [{"RegistryNumber": "0", "NameOfSubstance": "Sesquiterpenes"}], "MeshHeadingList": [{"QualifierName": [], "DescriptorName": "Animals"}, {"QualifierName": ["drug effects"], "DescriptorName": "Histamine Release"}, {"QualifierName": ["drug therapy"], "DescriptorName": "Hypersensitivity"}, {"QualifierName": [], "DescriptorName": "Male"}, {"QualifierName": ["drug effects", "metabolism"], "DescriptorName": "Mast Cells"}, {"QualifierName": ["drug effects"], "DescriptorName": "Passive Cutaneous Anaphylaxis"}, {"QualifierName": ["chemistry"], "DescriptorName": "Plants, Medicinal"}, {"QualifierName": [], "DescriptorName": "Rats"}, {"QualifierName": [], "DescriptorName": "Rats, Inbred Strains"}, {"QualifierName": ["isolation & purification", "pharmacology"], "DescriptorName": "Sesquiterpenes"}]}, "PubmedData": {"ReferenceList": [], "History": [{"Year": "1991", "Month": "12", "Day": "1"}, {"Year": "1991", "Month": "12", "Day": "1", "Hour": "0", "Minute": "1"}, {"Year": "1991", "Month": "12", "Day": "1", "Hour": "0", "Minute": "0"}], "PublicationStatus": "ppublish", "ArticleIdList": ["1726075", "10.1248/cpb.39.3272"]}}]}