{"PubmedBookArticle": [], "PubmedArticle": [{"MedlineCitation": {"OtherAbstract": [], "CitationSubset": ["IM"], "OtherID": [], "GeneralNote": [], "SpaceFlightMission": [], "KeywordList": [], "InvestigatorList": [], "PMID": "4065392", "DateCompleted": {"Year": "1986", "Month": "01", "Day": "21"}, "DateRevised": {"Year": "2022", "Month": "03", "Day": "17"}, "Article": {"Language": ["eng"], "ArticleDate": [], "ELocationID": [], "Journal": {"ISSN": "0265-203X", "JournalIssue": {"Volume": "2", "Issue": "4", "PubDate": {"Year": "1985", "Season": "Oct-Dec"}}, "Title": "Food additives and contaminants", "ISOAbbreviation": "Food Addit Contam"}, "ArticleTitle": "Ochratoxin A contamination of cereals in an area of high incidence of Balkan endemic nephropathy in Bulgaria.", "Pagination": {"StartPage": "267", "EndPage": "270", "MedlinePgn": "267-70"}, "Abstract": {"AbstractText": ["In an effort to provide further evidence for the hypothesis that a mycotoxin is involved in the aetiology of Balkan endemic nephropathy and that the latter is associated with the occurrence of urinary system tumours, a survey was made of ochratoxin A contamination of cereal samples from an area of Bulgaria where both endemic nephropathy and urinary system tumours are prevalent and from non-endemic areas. In all, 130 samples of beans, maize and wheat flour were analysed. Ochratoxin A levels were 16.7% (25-27 micrograms/kg) in bean samples from the endemic area and 7.1% (25-50 micrograms/kg) in those from the control areas: in maize samples, the levels were 27.3% (25-35 micrograms/kg) and 9.0% (10-25 micrograms/kg), respectively."]}, "AuthorList": [{"Identifier": [], "AffiliationInfo": [], "LastName": "Petkova-Bocharova", "ForeName": "T", "Initials": "T"}, {"Identifier": [], "AffiliationInfo": [], "LastName": "Castegnaro", "ForeName": "M", "Initials": "M"}], "PublicationTypeList": ["Journal Article"]}, "MedlineJournalInfo": {"Country": "England", "MedlineTA": "Food Addit Contam", "NlmUniqueID": "8500474", "ISSNLinking": "0265-203X"}, "ChemicalList": [{"RegistryNumber": "0", "NameOfSubstance": "Ochratoxins"}, {"RegistryNumber": "1779SX6LUY", "NameOfSubstance": "ochratoxin A"}], "MeshHeadingList": [{"QualifierName": ["chemically induced"], "DescriptorName": "Balkan Nephropathy"}, {"QualifierName": ["analysis"], "DescriptorName": "Edible Grain"}, {"QualifierName": ["analysis"], "DescriptorName": "Fabaceae"}, {"QualifierName": ["analysis"], "DescriptorName": "Flour"}, {"QualifierName": [], "DescriptorName": "Food Microbiology"}, {"QualifierName": [], "DescriptorName": "Humans"}, {"QualifierName": ["chemically induced"], "DescriptorName": "Nephritis, Interstitial"}, {"QualifierName": ["adverse effects", "analysis"], "DescriptorName": "Ochratoxins"}, {"QualifierName": [], "DescriptorName": "Plants, Medicinal"}, {"QualifierName": ["analysis"], "DescriptorName": "Triticum"}, {"QualifierName": ["analysis"], "DescriptorName": "Zea mays"}]}, "PubmedData": {"ReferenceList": [], "History": [{"Year": "1985", "Month": "10", "Day": "1"}, {"Year": "1985", "Month": "10", "Day": "1", "Hour": "0", "Minute": "1"}, {"Year": "1985", "Month": "10", "Day": "1", "Hour": "0", "Minute": "0"}], "PublicationStatus": "ppublish", "ArticleIdList": ["4065392", "10.1080/02652038509373555"]}}]}