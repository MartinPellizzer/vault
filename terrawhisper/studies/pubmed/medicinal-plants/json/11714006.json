{"PubmedArticle": [{"MedlineCitation": {"SpaceFlightMission": [], "OtherID": [], "InvestigatorList": [], "OtherAbstract": [], "CitationSubset": ["IM"], "KeywordList": [], "GeneralNote": [], "PMID": "11714006", "DateCompleted": {"Year": "2001", "Month": "12", "Day": "14"}, "DateRevised": {"Year": "2004", "Month": "11", "Day": "17"}, "Article": {"ArticleDate": [], "Language": ["eng"], "ELocationID": [], "Journal": {"ISSN": "0309-3913", "JournalIssue": {"Volume": "29", "Issue": "3-4", "PubDate": {"Year": "2000", "Season": "Sep-Dec"}}, "Title": "African journal of medicine and medical sciences", "ISOAbbreviation": "Afr J Med Med Sci"}, "ArticleTitle": "Antimicrobial activity of Mallotus oppositifolium extractives.", "Pagination": {"StartPage": "281", "EndPage": "283", "MedlinePgn": "281-3"}, "Abstract": {"AbstractText": ["A bioactivity monitored phytochemical examination of the morphological parts of Mallotus oppositifolium utilizing the hole-in-plate bioassay procedure against gram-positive, gram-negative bacteria and fungal isolates resulted in the location of significant antimicrobial activity in the acidic fraction (HAF) of the hexane extract of the powdered leaves. Minimum inhibitory concentration (MIC) and minimum bactericidal concentration (MBC) values were 32.5 microg/ml and 65 microg/ml against Pseudomonas aeruginosa NCTC 6750 and 25 microg/ml and 50 microg/ml against Staphylococcus aureus NCTC 6571 respectively. These activities were found comparable with standard drugs."]}, "AuthorList": [{"AffiliationInfo": [{"Identifier": [], "Affiliation": "Dept of Pharmacognosy, Faculty of Pharmacy, University of Ibadan, Nigeria."}], "Identifier": [], "LastName": "Ogundipe", "ForeName": "O O", "Initials": "OO"}, {"AffiliationInfo": [], "Identifier": [], "LastName": "Moody", "ForeName": "J O", "Initials": "JO"}, {"AffiliationInfo": [], "Identifier": [], "LastName": "Fakeye", "ForeName": "T O", "Initials": "TO"}, {"AffiliationInfo": [], "Identifier": [], "LastName": "Ladipo", "ForeName": "O B", "Initials": "OB"}], "PublicationTypeList": ["Journal Article"]}, "MedlineJournalInfo": {"Country": "Nigeria", "MedlineTA": "Afr J Med Med Sci", "NlmUniqueID": "7801013", "ISSNLinking": "0309-3913"}, "ChemicalList": [{"RegistryNumber": "0", "NameOfSubstance": "Plant Extracts"}], "MeshHeadingList": [{"QualifierName": [], "DescriptorName": "Biological Assay"}, {"QualifierName": ["methods"], "DescriptorName": "Chemical Fractionation"}, {"QualifierName": [], "DescriptorName": "Chromatography, Thin Layer"}, {"QualifierName": [], "DescriptorName": "Drug Evaluation, Preclinical"}, {"QualifierName": ["drug effects"], "DescriptorName": "Fungi"}, {"QualifierName": ["drug effects"], "DescriptorName": "Gram-Negative Bacteria"}, {"QualifierName": ["drug effects"], "DescriptorName": "Gram-Positive Bacteria"}, {"QualifierName": [], "DescriptorName": "Humans"}, {"QualifierName": [], "DescriptorName": "Medicine, African Traditional"}, {"QualifierName": [], "DescriptorName": "Microbial Sensitivity Tests"}, {"QualifierName": [], "DescriptorName": "Nigeria"}, {"QualifierName": ["chemistry"], "DescriptorName": "Plant Extracts"}, {"QualifierName": ["chemistry", "physiology"], "DescriptorName": "Plants, Medicinal"}]}, "PubmedData": {"ReferenceList": [], "History": [{"Year": "2001", "Month": "11", "Day": "21", "Hour": "10", "Minute": "0"}, {"Year": "2002", "Month": "1", "Day": "5", "Hour": "10", "Minute": "1"}, {"Year": "2001", "Month": "11", "Day": "21", "Hour": "10", "Minute": "0"}], "PublicationStatus": "ppublish", "ArticleIdList": ["11714006"]}}], "PubmedBookArticle": []}