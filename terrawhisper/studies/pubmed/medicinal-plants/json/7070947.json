{"PubmedBookArticle": [], "PubmedArticle": [{"MedlineCitation": {"OtherAbstract": [], "CitationSubset": ["IM"], "OtherID": [], "GeneralNote": [], "SpaceFlightMission": [], "KeywordList": [], "InvestigatorList": [], "PMID": "7070947", "DateCompleted": {"Year": "1982", "Month": "06", "Day": "14"}, "DateRevised": {"Year": "2019", "Month": "11", "Day": "11"}, "Article": {"Language": ["eng"], "ArticleDate": [], "ELocationID": [], "Journal": {"ISSN": "0167-6555", "JournalIssue": {"Volume": "4", "Issue": "1", "PubDate": {"Year": "1982", "Month": "Feb", "Day": "19"}}, "Title": "Pharmaceutisch weekblad. Scientific edition", "ISOAbbreviation": "Pharm Weekbl Sci"}, "ArticleTitle": "Separation of valtrate and isovaltrate by means of preparative liquid chromatography.", "Pagination": {"StartPage": "21", "EndPage": "24", "MedlinePgn": "21-4"}, "Abstract": {"AbstractText": ["In this study valtrate and isovaltrate, two isomeric valepotriates present in various Valeriana species, were separated on a preparative scale with a Waters preparative Liquid Chromatography/500 A system. A rapid separation was achieved with the solvent methylene chloride-n-propanol-acetone (99 + 0.5 + 0.5). Extensive use of the recycle mode of the instrument resulted in a considerable reduction of the amount of solvent used, without affecting the purity and the recovery (greater than 90%) of the separated components. With the method presented in this communication 1 gram of a mixture of valtrate and isovaltrate was separated in 20 minutes using 2.1 liters of solvent."]}, "AuthorList": [{"Identifier": [], "AffiliationInfo": [], "LastName": "Hazelhoff", "ForeName": "B", "Initials": "B"}, {"Identifier": [], "AffiliationInfo": [], "LastName": "Jellema", "ForeName": "R", "Initials": "R"}, {"Identifier": [], "AffiliationInfo": [], "LastName": "Grobben", "ForeName": "H", "Initials": "H"}, {"Identifier": [], "AffiliationInfo": [], "LastName": "Malingr\u00e9", "ForeName": "T M", "Initials": "TM"}], "PublicationTypeList": ["Journal Article"]}, "MedlineJournalInfo": {"Country": "Netherlands", "MedlineTA": "Pharm Weekbl Sci", "NlmUniqueID": "7907992", "ISSNLinking": "0167-6555"}, "ChemicalList": [{"RegistryNumber": "0", "NameOfSubstance": "Iridoids"}, {"RegistryNumber": "0", "NameOfSubstance": "Pyrans"}, {"RegistryNumber": "0", "NameOfSubstance": "Solvents"}, {"RegistryNumber": "0", "NameOfSubstance": "valtrates (combination)"}], "MeshHeadingList": [{"QualifierName": ["methods"], "DescriptorName": "Chromatography, Liquid"}, {"QualifierName": [], "DescriptorName": "Iridoids"}, {"QualifierName": [], "DescriptorName": "Plants, Medicinal"}, {"QualifierName": ["isolation & purification"], "DescriptorName": "Pyrans"}, {"QualifierName": [], "DescriptorName": "Solvents"}, {"QualifierName": ["analysis"], "DescriptorName": "Valerian"}]}, "PubmedData": {"ReferenceList": [{"Reference": [{"Citation": "Biomed Mass Spectrom. 1979 Mar;6(3):124-8", "ArticleIdList": ["420917"]}, {"Citation": "Planta Med. 1979 Sep;37(1):84-6", "ArticleIdList": ["504481"]}], "ReferenceList": []}], "History": [{"Year": "1982", "Month": "2", "Day": "19"}, {"Year": "1982", "Month": "2", "Day": "19", "Hour": "0", "Minute": "1"}, {"Year": "1982", "Month": "2", "Day": "19", "Hour": "0", "Minute": "0"}], "PublicationStatus": "ppublish", "ArticleIdList": ["7070947", "10.1007/BF02112353"]}}]}