{"PubmedArticle": [{"MedlineCitation": {"InvestigatorList": [], "SpaceFlightMission": [], "CitationSubset": ["IM"], "KeywordList": [], "GeneralNote": [], "OtherAbstract": [], "OtherID": [], "PMID": "24005922", "DateCompleted": {"Year": "2014", "Month": "02", "Day": "07"}, "DateRevised": {"Year": "2019", "Month": "08", "Day": "23"}, "Article": {"Language": ["eng"], "ELocationID": [], "ArticleDate": [], "Journal": {"ISSN": "1678-4464", "JournalIssue": {"Volume": "29", "Issue": "8", "PubDate": {"Year": "2013", "Month": "Aug"}}, "Title": "Cadernos de saude publica", "ISOAbbreviation": "Cad Saude Publica"}, "ArticleTitle": "Particulate matter and hospital admission due to arterial hypertension in a medium-sized Brazilian city.", "Pagination": {"StartPage": "1565", "EndPage": "1571", "MedlinePgn": "1565-71"}, "Abstract": {"AbstractText": ["This study aims to assess the effects air pollution on hospitalization due to hypertension. An ecological study was conducted using data regarding hospitalizations due to hypertension in S\u00e3o Jos\u00e9 dos Campos, State of S\u00e3o Paulo, Brazil between January 1, 2007 and December 31, 2010. The pollutants analyzed by this study were particulate matter, sulfur dioxide and ozone. A significant association between exposure to particulate matter and hospitalization due to hypertension was identified using Poisson regression with lags of up to four days. Relative risk of hospital admission ranged between 1,009 and 1,019. A 10\u03bcg/m\u00b3 increase in concentration of particulate matter was associated with a 13% increase in risk of hospitalization."]}, "AuthorList": [{"AffiliationInfo": [{"Identifier": [], "Affiliation": "Universidade de Taubat\u00e9, Taubat\u00e9, Brasil. luiz.nascimento@unitau.br"}], "Identifier": [], "LastName": "Nascimento", "ForeName": "Luiz Fernando Costa", "Initials": "LF"}, {"AffiliationInfo": [], "Identifier": [], "LastName": "Francisco", "ForeName": "Juliana Batrov", "Initials": "JB"}], "PublicationTypeList": ["Journal Article", "Research Support, Non-U.S. Gov't"]}, "MedlineJournalInfo": {"Country": "Brazil", "MedlineTA": "Cad Saude Publica", "NlmUniqueID": "8901573", "ISSNLinking": "0102-311X"}, "ChemicalList": [{"RegistryNumber": "0", "NameOfSubstance": "Air Pollutants"}, {"RegistryNumber": "0", "NameOfSubstance": "Particulate Matter"}, {"RegistryNumber": "0UZA3422Q4", "NameOfSubstance": "Sulfur Dioxide"}, {"RegistryNumber": "66H7ZZK23N", "NameOfSubstance": "Ozone"}], "MeshHeadingList": [{"QualifierName": ["analysis", "toxicity"], "DescriptorName": "Air Pollutants"}, {"QualifierName": ["epidemiology"], "DescriptorName": "Brazil"}, {"QualifierName": ["adverse effects", "analysis"], "DescriptorName": "Environmental Exposure"}, {"QualifierName": ["statistics & numerical data"], "DescriptorName": "Hospitalization"}, {"QualifierName": [], "DescriptorName": "Humans"}, {"QualifierName": ["epidemiology", "etiology"], "DescriptorName": "Hypertension"}, {"QualifierName": ["analysis", "toxicity"], "DescriptorName": "Ozone"}, {"QualifierName": ["analysis", "toxicity"], "DescriptorName": "Particulate Matter"}, {"QualifierName": ["analysis", "toxicity"], "DescriptorName": "Sulfur Dioxide"}]}, "PubmedData": {"ReferenceList": [], "History": [{"Year": "2013", "Month": "9", "Day": "6", "Hour": "6", "Minute": "0"}, {"Year": "2013", "Month": "9", "Day": "6", "Hour": "6", "Minute": "0"}, {"Year": "2014", "Month": "2", "Day": "8", "Hour": "6", "Minute": "0"}], "PublicationStatus": "ppublish", "ArticleIdList": ["24005922", "10.1590/0102-311x00127612", "S0102-311X2013000800009"]}}], "PubmedBookArticle": []}